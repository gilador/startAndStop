<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:top="0dp"
        android:left="0dp"
        android:bottom="0dp"
        android:right="0dp">

        <shape android:shape="oval">

            <solid android:color="@color/yellow" />

            <size
                android:width="60dp"
                android:height="60dp" />
        </shape>
    </item>

    <item
        android:top="25dp"
        android:left="25dp"
        android:bottom="25dp"
        android:right="25dp">

        <rotate xmlns:android="http://schemas.android.com/apk/res/android"
            android:fromDegrees="90"
            android:toDegrees="90">
        <shape
            android:shape="line">
            <stroke
                android:width="1dp"
                android:color="#ff000000"/>
        </shape>
        </rotate>
    </item>

    <item
        android:top="25dp"
        android:left="25dp"
        android:bottom="25dp"
        android:right="25dp">

        <shape
            android:shape="line">
            <stroke
                android:width="1dp"
                android:color="#ff000000"/>
        </shape>
    </item>

</layer-list>